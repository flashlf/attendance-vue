{
    "name": "singleui-remoteapp-boilerplate",
    "private": true,
    "version": "0.0.0",
    "description": "Boilerplate for Remote App Single UI",
    "type": "module",
    "author": "Rivaldy Firmansyah",
    "scripts": {
        "preview": "vite build --mode localhost --watch & vite preview --port 9000 --strictPort",
        "build:local": "vite build --mode localhost",
        "build:development": "vite build --mode development",
        "build:staging": "vite build --mode staging",
        "build:production": "vite build --mode production",
        "danger:lint-report": "NODE_ENV=production eslint . --format checkstyle --output-file reports/lint-results.xml --quiet || true",
        "danger:ci": "danger ci --failOnErrors",
        "prettier": "prettier --write .",
        "typeheck": "vue-tsc --noEmit && vite build",
        "lint": "eslint . --fix",
        "prepare": "husky",
        "dev": "vite"
    },
    "lint-staged": {
        "*.{js,ts,vue}": [
            "npm run lint"
        ]
    },
